<h1>Create a User</h1>

<%= form_for @user do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %><br>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br><br>



  <%= f.submit %>
<% end %>
