<div class="container">
  <div class="">
    <p class="display-4">Image library</p>
  </div>

  <div class="row">
  <% @images.each do |image| %>
  <div class="mr-2">
    <a href="<%= image_path(image) %>"><img src="<%= image.name %>" alt="<%= image.mood.name %>" width="200"></a>
    <div class="">
      <p class="badge badge-warning"><%= image.mood.name %></p>
    </div>

    </div>
  <% end %>
  </div>
</div>
